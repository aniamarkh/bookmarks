{
  "manifest_version": 3,
  "name": "kartotek",
  "description": "Manage bookmarks extension",
  "version": "1",
  "icons": {
    "128": "logo-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "favicon"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}